gfx/impact/awe_blood_drop_stain0
{
	entityMergable
	surfaceparm nonsolid
	surfaceparm trans
	polygonOffset2
    {
        map gfx/impact/awe_blood_drop_stain0
        blendfunc blend
        rgbGen vertex
    nextbundle
		map $lightmap
    }
    {
		perlight
        map gfx/impact/awe_blood_drop_stain0
        blendfunc GL_SRC_ALPHA GL_ONE
        rgbGen vertex
    nextbundle
		map $dlight
    }
}

